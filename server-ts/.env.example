# Server configuration
PORT=3000
NODE_ENV=development
CORS_ORIGINS = "http://localhost:4000"


# Database configuration (example for PostgreSQL)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=myapp
DB_USER=postgres
DB_PASSWORD=password

# JWT configuration
JWT_SECRET=your_jwt_secret_key
