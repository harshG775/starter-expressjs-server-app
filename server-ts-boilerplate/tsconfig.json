{
    "extends": "./tsconfig.base.json",
    "compilerOptions": {
        "typeRoots": ["./node_modules/@types", "./src/@types"],

        "strictFunctionTypes": true,
        "noImplicitAny": true,
        "strictBindCallApply": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,

    }
}
